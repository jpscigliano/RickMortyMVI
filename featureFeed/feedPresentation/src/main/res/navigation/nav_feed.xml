<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:id="@+id/nav_feed"
  app:startDestination="@id/fragmentList">

  <fragment
    android:id="@+id/fragmentList"
    android:name="com.testme.feedpresentation.characterList.ListFragment"
    tools:layout="@layout/fragment_characters_list">
    <action android:id="@+id/actionCharactersListToCharacterDetail"
      app:destination="@+id/fragmentCharacterDetail"
      />
  </fragment>


  <fragment
    android:id="@+id/fragmentCharacterDetail"
    android:name="com.testme.feedpresentation.characterDetail.CharacterDetailFragment"
    tools:layout="@layout/fragment_character_detail" />
</navigation>